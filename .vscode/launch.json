{
	"version": "0.1.0",
	"configurations": [
		{
			"name": "Launch tests",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
			"stopOnEntry": false,
			"args": [ "out/tests/tests" ],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": ["--nolazy"],
			"env": {
				"NODE_ENV": "development"
			},
			"externalConsole": false,
			"sourceMaps": false,
			"outDir": null
		}
	]
}
